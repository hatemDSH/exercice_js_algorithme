ALGORITHM algorithm_plus_frand
VAR
    val1 , val2 : INTEGER

BEGIN
    write(" donner un entier  val1 = ")
    Read(val1)
    write(" donner un entier  val1 = ")
    Read(val2)

    IF (val1 > val2 ) THEN
        write("  valeur 1 est plus grand que valeur 2 ")
    ELSE
         write("  valeur 2 est plus grand que valeur 1 ")
    END_IF
END


